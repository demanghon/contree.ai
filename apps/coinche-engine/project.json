{
    "name": "coinche-engine",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/coinche-engine/src",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": ["{projectRoot}/target"],
            "options": {
                "command": "unset CONDA_PREFIX && . $HOME/.cargo/env && maturin build",
                "cwd": "apps/coinche-engine"
            }
        },
        "develop": {
            "executor": "nx:run-commands",
            "options": {
                "command": "unset CONDA_PREFIX && . $HOME/.cargo/env && maturin develop",
                "cwd": "apps/coinche-engine"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cargo test",
                "cwd": "apps/coinche-engine"
            }
        }
    }
}